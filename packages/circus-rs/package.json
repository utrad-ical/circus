{
  "name": "@utrad-ical/circus-rs",
  "version": "0.26.0-alpha.6",
  "scripts": {
    "start": "node ./server.js",
    "debug": "nodemon --inspect ./server.js",
    "devserver": "webpack-dev-server",
    "test": "jest",
    "build": "webpack",
    "watch": "webpack --watch",
    "build-npm": "tsc && cp src/browser/image-source/volume-rendering-image-source/*.{vert,frag} lib/browser/image-source/volume-rendering-image-source",
    "clean": "rimraf dist",
    "docs": "jekyll build --source=./docs --dest=./docs/_site && cp -r ./dist ./docs/_site",
    "docserver": "jekyll serve --source=./docs --dest=./docs/_site",
    "lint": "eslint --ext .js,.ts src",
    "prettier": "prettier src/**/*.{ts,js,less} test/**/*.{ts,js} --write"
  },
  "repository": "https://github.com/utrad-ical/circus-rs",
  "license": "MIT",
  "main": "lib/browser/index.js",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@utrad-ical/circus-lib": "file:../circus-lib",
    "async-cache": "^1.1.0",
    "css-loader": "^0.28.7",
    "dicom-parser": "^1.7.5",
    "extend": "^3.0.2",
    "gl-matrix": "^2.4.0",
    "jpeg-lossless-decoder-js": "^2.0.2",
    "json5": "^0.5.1",
    "koa": "^2.11.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.2.1",
    "minimist": "^1.2.0",
    "multi-integer-range": "^4.0.6",
    "pngjs": "^3.3.3",
    "style-loader": "^0.19.1",
    "three": "^0.98.0",
    "tinycolor2": "^1.4.1",
    "tslib": "^1.11.1",
    "validator": "^7.0.0",
    "winston": "^3.2.1"
  },
  "nodemonConfig": {
    "ext": "ts,js,json"
  },
  "prettier": {
    "singleQuote": true
  }
}
