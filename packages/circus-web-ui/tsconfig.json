{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "es2019"],
    "jsx": "react",
    "allowJs": true,
    "paths": {
      "components/*": ["circus-web-ui/src/components/*"],
      "pages/*": ["circus-web-ui/src/pages/*"],
      "utils/*": ["circus-web-ui/src/utils/*"],
      "store": ["circus-web-ui/src/store"],
      "store/*": ["circus-web-ui/src/store/*"],
      "types/*": ["circus-web-ui/src/types/*"]
    },
    "rootDir": "./src"
  },
  "include": ["src"]
}
